{
  "manifest_version": 2,

  "name": "makeMeFree",
  "description": "This extension aim to make you able to read content for private users",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "click here"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
